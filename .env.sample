# The connection string for your database.
# Format: dialect+driver://username:password@host:port/database
DATABASE_URL=

# Your API key for the Google Gemini service.
GEMINI_APIKEY=

# The base file path for local data processing.
BASE_PATH=

# A secret key used for signing sessions and security purposes.
# Should be a long, randomly generated string.
SECRET_KEY=

# --- Google OAuth Credentials ---
# Found in your Google Cloud Console for the OAuth 2.0 Client ID.
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# The callback URL registered in your Google Cloud Console.
# This is where Google redirects after authentication.
GOOGLE_REDIRECT_URI=

# The URI to redirect to on the frontend after a successful login.
FRONTEND_REDIRECT_URI=